# Environment Variables Template
# Copy this file to .env.development or .env.production and fill in your actual values

# Environment
NODE_ENV=development
EXPO_PUBLIC_ENV=development
AUTO_CONFIRM_EMAIL=true

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API URLs
EXPO_PUBLIC_NEST_API_URL=http://192.168.1.110:3000
EXPO_PUBLIC_MAP_API_URL=http://192.168.1.110:3004
EXPO_PUBLIC_REALTIME_URL=http://192.168.1.110:3100

# Development Settings
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true
EXPO_PUBLIC_MOCK_DATA_WHEN_NO_AUTH=true

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Production Settings (when NODE_ENV=production)
# NODE_ENV=production
# EXPO_PUBLIC_ENV=production
# AUTO_CONFIRM_EMAIL=false
# EXPO_PUBLIC_NEST_API_URL=https://api.yourdomain.com
# EXPO_PUBLIC_MAP_API_URL=https://map-api.yourdomain.com
# EXPO_PUBLIC_REALTIME_URL=https://realtime.yourdomain.com
# EXPO_PUBLIC_ENABLE_DEV_TOOLS=false
# EXPO_PUBLIC_MOCK_DATA_WHEN_NO_AUTH=false
